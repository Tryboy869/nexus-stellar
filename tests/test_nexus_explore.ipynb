            state=list(concept_vector),
            properties={
                'thought_id': thought_id,
                'label': label,
                'attention': 0.0,
                'activation': 0.0,
                'associations': []
            }
        )
    
    def activate(self, strength=1.0):
        """Active la pens√©e"""
        self.activation = strength
        self.entity.properties['activation'] = strength
    
    def associate_with(self, other_thought):
        """Cr√©e association avec autre pens√©e"""
        self.entity.properties['associations'].append(other_thought.id)

