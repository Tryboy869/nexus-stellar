# Force de conscience
def consciousness_force(e1, e2):
    """Force d'association et attention"""
    
    # Similarit√© s√©mantique
    v1 = np.array(e1.state)
    v2 = np.array(e2.state)
    
    # Cosine similarity
    similarity = np.dot(v1, v2) / (np.linalg.norm(v1) * np.linalg.norm(v2) + 1e-8)
    
    # Activation influence attention
    activation_boost = e1.properties['activation'] + e2.properties['activation']
    
    # Force = similarit√© √ó activation
    force_strength = similarity * (1 + activation_boost)
    
    diff = e2.state - e1.state
    return diff * force_strength * 0.1

