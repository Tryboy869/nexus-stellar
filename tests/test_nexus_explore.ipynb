class QuantumQubit:
    """Qubit simul√© avec superposition"""
    
    def __init__(self, qubit_id, initial_state=None):
        self.id = qubit_id
        
        if initial_state is None:
            # Superposition √©gale |0‚ü© + |1‚ü©
            theta = random.uniform(0, np.pi)
            self.prob_0 = np.cos(theta)**2
            self.prob_1 = np.sin(theta)**2
        else:
            self.prob_0, self.prob_1 = initial_state
        
        # Normalisation
        total = self.prob_0 + self.prob_1
        self.prob_0 /= total
        self.prob_1 /= total
        
        # Entit√©s pour Nexus
        self.entity = Entity(
            state=[self.prob_0, self.prob_1],
            properties={'qubit_id': qubit_id, 'entangled_with': []}
        )
    
    def measure(self):
        """Mesure du qubit (collapse)"""
        return 0 if random.random() < self.prob_0 else 1
    
    def entangle_with(self, other_qubit):
        """Intrication avec autre qubit"""
        self.entity.properties['entangled_with'].append(other_qubit.id)
        other_qubit.entity.properties['entangled_with'].append(self.id)

