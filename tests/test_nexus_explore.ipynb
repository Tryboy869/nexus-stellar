class CodeFunction:
    """Fonction auto-√©volutive"""
    
    def __init__(self, func_id, genome):
        self.id = func_id
        self.genome = genome  # [coef_a, coef_b, coef_c]
        self.performance = 0.0
        self.generation = 0
        
        self.entity = Entity(
            state=list(genome),
            properties={
                'func_id': func_id,
                'performance': 0.0,
                'generation': 0
            }
        )
    
    def execute(self, x):
        """Ex√©cute la fonction f(x) = a*x¬≤ + b*x + c"""
        a, b, c = self.genome
        return a * x**2 + b * x + c
    
    def evaluate(self, test_data):
        """√âvalue performance sur donn√©es test"""
        # Objectif: Approcher fonction cible y = x¬≤
        errors = []
        for x, y_target in test_data:
            y_pred = self.execute(x)
            errors.append((y_pred - y_target)**2)
        
        # Performance = inverse de l'erreur
        mse = np.mean(errors)
        self.performance = 1.0 / (1.0 + mse)
        self.entity.properties['performance'] = self.performance
        return self.performance
    
    def mutate(self, other_func):
        """Mutation g√©n√©tique"""
        # Crossover + mutation
        new_genome = []
        for i in range(3):
            # M√©lange parental
            if random.random() < 0.5:
                gene = self.genome[i]
            else:
                gene = other_func.genome[i]
            
            # Mutation al√©atoire
            if random.random() < 0.1:
                gene += random.gauss(0, 0.1)
            
            new_genome.append(gene)
        
        self.genome = tuple(new_genome)
        self.entity.state = list(new_genome)
        self.generation += 1
        self.entity.properties['generation'] = self.generation

